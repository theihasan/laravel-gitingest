{
    "name": "theihasan/laravel-gitingest",
    "description": "Convert GitHub repositories into LLM-friendly text files into your Laravel applications",
    "keywords": [
        "Ihasan",
        "laravel",
        "laravel-gitingest"
    ],
    "homepage": "https://github.com/theihasan/laravel-gitingest",
    "license": "MIT",
    "authors": [
        {
            "name": "Md Abul Hassan",
            "email": "imabulhasan99@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^10.0||^11.0||^12.0",
        "react/http": "^1.9",
        "react/filesystem": "^0.1",
        "react/stream": "^1.3",
        "guzzlehttp/guzzle": "^7.8",
        "yethee/tiktoken": "^0.10"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "larastan/larastan": "^2.9||^3.0",
        "orchestra/testbench": "^9.0",
        "pestphp/pest": "^2.0||^3.0",
        "pestphp/pest-plugin-arch": "^2.5||^3.0",
        "pestphp/pest-plugin-laravel": "^2.0||^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1||^2.0",
        "phpstan/phpstan-phpunit": "^1.3||^2.0",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ray": "^1.35"
    },
    "autoload": {
        "psr-4": {
            "Ihasan\\LaravelGitingest\\": "src/",
            "Ihasan\\LaravelGitingest\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ihasan\\LaravelGitingest\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "wyrihaximus/composer-update-bin-autoload-path": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ihasan\\LaravelGitingest\\LaravelGitingestServiceProvider"
            ],
            "aliases": {
                "LaravelGitingest": "Ihasan\\LaravelGitingest\\Facades\\LaravelGitingest"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
